<T3DataStructure>
    <meta type="array">
        <langChildren type="integer">0</langChildren>
        <langDisable type="integer">1</langDisable>
    </meta>
    <sheets>
        <sheet1>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:faq/Resources/Private/Language/locallang.xlf:flexform.topMode.title</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.faq.itemsPerPage>
                        <TCEforms>
                            <label>LLL:EXT:faq/Resources/Private/Language/locallang.xlf:flexform.itemsPerPage</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>int</eval>
                                <range>
                                    <lower>1</lower>
                                    <upper>1000</upper>
                                </range>
                                <default>10</default>
                            </config>
                        </TCEforms>
                    </settings.faq.itemsPerPage>
                    <settings.faq.enablePagination>
                        <TCEforms>
                            <label>Enable pagination</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.faq.enablePagination>
                    <settings.faq.addSchmemaOrgHeader>
                    	<TCEforms>
                    		<label>LLL:EXT:faq/Resources/Private/Language/locallang.xlf:flexform.addSchemaOrgHeader</label>
                    		<config>
                    			<type>check</type>
                    		</config>
                    	</TCEforms>
                    </settings.faq.addSchmemaOrgHeader>
                </el>
            </ROOT>
        </sheet1>
    </sheets>
</T3DataStructure>
